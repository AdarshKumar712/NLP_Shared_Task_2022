u0629\u060c \u0639\u0644\u0649 \u0633\u0631\u0639\u0629 \u0627\u0644\u0625\u0646\u062a\u0647\u0627\u0621 \u0645\u0646 \u0631\u0635\u0641 \u0648\u062a\u0645\u0647\u064a\u062f \u0648\u0625\u0639\u0627\u062f\u0629 \u0627\u0644\u0634\u064a\u0621 \u0644\u0623\u0635\u0644\u0647 \u0628\u0643\u0627\u0641\u0629 \u0627\u0644\u0634\u0648\u0627\u0631\u0639 \u0639\u0644\u0649 \u0645\u0633\u062a\u0648\u0649 \u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u0628\u0639\u062f \u0627\u0644\u0625\u0646\u062a\u0647\u0627\u0621 \u0627\u0644\u062a\u0627\u0645 \u0645\u0646 \u062a\u0648\u0635\u064a\u0644 \u0643\u0627\u0641\u0629 \u0627\u0644\u0645\u0631\u0627\u0641\u0642 \"\u063a\u0627\u0632 \u0648\u0643\u0647\u0631\u0628\u0627\u0621 \u0648\u0645\u064a\u0627\u0647 \u0648\u062a\u0644\u064a\u0641\u0648\u0646\u0627\u062a\"\u060c \u0644\u062a\u0648\u0641\u064a\u0631 \u0627\u0644\u0633\u064a\u0648\u0644\u0629 \u0627\u0644\u0645\u0631\u0648\u0631\u064a\u0629 \u0648\u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0627\u0644\u0645\u0638\u0647\u0631 \u0627\u0644\u062c\u0645\u0627\u0644\u064a \u0644\u0644\u0634\u0648\u0627\u0631\u0639.\n\n\u062c\u0627\u0621 \u0630\u0644\u0643 \u062e\u0644\u0627\u0644 \u062a\u0641\u0642\u062f\u0647 \u0644\u0623\u0639\u0645\u0627\u0644 \u0631\u0635\u0641 \u0634\u0627\u0631\u0639 \u0627\u0644\u0645\u0624\u064a\u062f \u0628\u062d\u0649 \u062b\u0627\u0646 \u0637\u0646\u0637\u0627 \u0628\u0637\u0648\u0644 300 \u0645 \u0636\u0645\u0646 \u062e\u0637\u0629 \u0631\u062f \u0627\u0644\u0634\u0626 \u0644\u0623\u0635\u0644\u0647\u060c \u0645\u0648\u0636\u062d\u0627 \u0623\u0646 \u062e\u0637\u0629 \u0631\u062f \u0627\u0644\u0634\u0626 \u0644\u0623\u0635\u0644\u0647 \u0627\u0644\u0645\u0639\u062a\u0645\u062f\u0629 \u0644\u062d\u0649 \u062b\u0627\u0646 \u0637\u0646\u0637\u0627 \u062a\u0642\u062f\u0631 \u0628\u0645\u0628\u0644\u063a \u0645\u0644\u064a\u0648\u0646 \u0648\u0646\u0635\u0641 \u062c\u0646\u064a\u0647 \u0644\u0631\u0635\u0641 7 \u0634\u0648\u0627\u0631\u0639.\n\n\u0648\u0623\u0634\u0627\u0631 \u0646\u0627\u0626\u0628 \u0645\u062d\u0627\u0641\u0638 \u0627\u0644\u063a\u0631\u0628\u064a\u0629\u060c \u0625\u0644\u0649 \u0645\u062a\u0627\u0628\u0639\u062a\u0647 \u0627\u0644\u0645\u0633\u062a\u0645\u0631\u0629\u060c \u0648\u0630\u0644\u0643 \u062a\u0646\u0641\u064a\u0630\u0627 \u0644\u062a\u0648\u062c\u064a\u0647\u0627\u062a \u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0637\u0627\u0631\u0642 \u0631\u062d\u0645\u064a \u0645\u062d\u0627\u0641\u0638 \u0627\u0644\u063a\u0631\u0628\u064a\u0629\u060c \u0644\u0644\u0648\u0642\u0648\u0641 \u0639\u0644\u0649 \u0645\u0639\u062f\u0644\u0627\u062a \u0648\u0646\u0633\u0628 \u0627\u0644\u062a\u0646\u0641\u064a\u0630 \u0648\u0627\u0644\u062a\u0623\u0643\u062f \u0645\u0646 \u0627\u0644\u0625\u0646\u062a\u0647\u0627\u0621 \u0645\u0646\u0647\u0627 \u0641\u064a \u0627\u0644\u0645\u0648\u0627\u0639\u064a\u062f \u0627\u0644\u0645\u062d\u062f\u062f\u0629 \u0644\u062e\u062f\u0645\u0629 \u0627\u0644\u0645\u0648\u0627\u0637\u0646\u064a\u0646 \u0648\u062c\u0646\u0649 \u062b\u0645\u0627\u0631 \u0627\u0644\u0645\u0634\u0631\u0648\u0639\u0627\u062a \u0627\u0644\u0642\u0648\u0645\u064a\u0629 \u0648\u0627\u0644\u062e\u062f\u0645\u064a\u0629 \u0627\u0644\u062c\u0627\u0631\u0649 \u062a\u0646\u0641\u064a\u0630\u0647\u0627 \u0628\u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629.\n\n\u0623\u0643\u062f \u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0623\u062d\u0645\u062f \u0639\u0637\u0627 \u0646\u0627\u0626\u0628 \u0645\u062d\u0627\u0641\u0638 \u0627\u0644\u063a\u0631\u0628\u064a\u0629 \u060c \u0627\u0644\u0627\u0646\u062a\u0647\u0627\u0621 \u0645\u0646 \u0643\u0627\u0641\u0629 \u0645\u0634\u0631\u0648\u0639\u0627\u062a \u0645\u064a\u0627\u0647 \u0627\u0644\u0634\u0631\u0628 \u0648\u0627\u0644\u0635\u0631\u0641 \u0627\u0644\u0635\u062d\u0649 \u0628\u062d\u0649 \u0623\u0648\u0644 \u0648\u062b\u0627\u0646 \u0637\u0646\u0637\u0627 \u062e\u0644\u0627\u0644 \u0627\u0644\u0623\u0634\u0647\u0631 \u0627\u0644\u062b\u0644\u0627\u062b\u0629 \u0627\u0644\u0642\u0627\u062f\u0645\u0629\u060c \u0645\u0634\u064a\u0631\u0627 \u0625\u0644\u0649 \u0623\u0646\u0647 \u062a\u0645 \u0627\u0644\u0627\u0646\u062a\u0647\u0627\u0621 \u0645\u0646 \u062a\u0646\u0641\u064a\u0630 \u0648\u062a\u0633\u0644\u064a\u0645 \u0639\u062f\u0629 \u0645\u0634\u0631\u0648\u0639\u0627\u062a \u0628\u0645\u062f\u0646 \u0648\u0642\u0631\u0649 \u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u062e\u0644\u0627\u0644 \u0627\u0644\u0639\u0627\u0645 \u0627\u0644\u062c\u0627\u0631\u064a.\n\n\u0648\u0623\u0636\u0627\u0641 \u0646\u0627\u0626\u0628 \u0645\u062d\u0627\u0641\u0638 \u0627\u0644\u063a\u0631\u0628\u064a\u0629\u060c\u0641\u0649 \u0628\u064a\u0627\u0646 \u0644\u0647 \u0627\u0644\u064a\u0648\u0645\u060c\u0623\u0646\u0647 \u0646\u0627\u0642\u0634 \u0643\u0627\u0641\u0629 \u0645\u0639\u0648\u0642\u0627\u062a \u0627\u0644\u0645\u0634\u0631\u0648\u0639\u0627\u062a \u0627\u0644\u0645\u062a\u0639\u062b\u0631\u0629 \u0648\u0627\u0644\u062c\u0627\u0631\u0649 \u062a\u0646\u0641\u064a\u0630\u0647\u0627 \u0628\u0642\u0637\u0627\u0639 \u0645\u064a\u0627\u0647 \u0627\u0644\u0634\u0631\u0628 \u0648\u0627\u0644\u0635\u0631\u0641 \u0627\u0644\u0635\u062d\u0649 \u0648\u0646\u0633\u0628 \u0627\u0644\u062a\u0646\u0641\u064a\u0630 \u0648\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u062a\u0639\u062b\u0631\u060c \u062c\u0627\u0621 \u0630\u0644\u0643 \u062e\u0644\u0627\u0644 \u0627\u062c\u062a\u0645\u0627\u0639\u0647 \u0645\u0639 \u0645\u0633\u0626\u0648\u0644\u0649 \u0634\u0631\u0643\u0629 \u0645\u064a\u0627\u0647 \u0627\u0644\u0634\u0631\u0628 \u0648\u0627\u0644\u0635\u0631\u0641 \u0627\u0644\u0635\u062d\u0649 \u0648\u0647\u064a\u0626\u0629 \u0627\u0644\u0637\u0631\u0642 \u0648\u0645\u062f\u064a\u0631\u064a\u0629 \u0627\u0644\u0637\u0631\u0642 \u0648\u0627\u0644\u0645\u0631\u0648\u0631 \u0648\u0631\u0626\u064a\u0633\u0627 \u062d\u0649 \u0623\u0648\u0644 \u0648\u062b\u0627\u0646 \u0637\u0646\u0637\u0627.\n\n\u0648\u0643\u0627\u0646 \u0637\u0627\u