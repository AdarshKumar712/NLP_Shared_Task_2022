9 \u0625\u0633\u0639\u0627\u0641 \u0644\u0646\u0642\u0644 \u0627\u0644\u0645\u0635\u0627\u0628\u064a\u0646 \u0625\u0644\u0649 \u0627\u0644\u0645\u0633\u062a\u0634\u0641\u064a \u0627\u0644\u0639\u0627\u0645.\n\n\n\n\u062a\u0644\u0642\u0649 \u0627\u0644\u0644\u0648\u0627\u0621 \u0623\u064a\u0645\u0646 \u0631\u0627\u0636\u064a \u0645\u062f\u064a\u0631 \u0623\u0645\u0646 \u0627\u0644\u0623\u0642\u0635\u0631 \u0625\u062e\u0637\u0627\u0631\u064b\u0627 \u0628\u0625\u0635\u0627\u0628\u0629 \u0643\u0644 \u0645\u0646: \"\u062d\u0633\u0646\u064a.\n\n\u0645.\u0627\" \u0664\u0663 \u0639\u0627\u0645\u064b\u0627 \u0645\u0642\u064a\u0645 \u0628\u0642\u0646\u0627\u060c \u0648\"\u0645\u0635\u0631\u064a\u0629. \u0627.\u062f\" \u0667\u0663 \u0639\u0627\u0645\u064b\u0627\u060c \u0645\u0642\u064a\u0645\u0629 \u0628\u0642\u0641\u0637\u060c \u0648\"\u0647\u0627\u062c\u0631.\u0639.\u0627\" \u0669\u0667 \u0639\u0627\u0645\u0651\u0627\u060c \u0648\"\u0645\u0646\u0649.\u0627.\u0627\" \u0665\u0666 \u0639\u0627\u0645\u064b\u0627\u060c \u0648\"\u0639\u0637\u064a\u0627\u062a.\u0639.\u0639\" \u0666\u0660 \u0639\u0627\u0645\u064b\u0627\u060c \u0648\" \u0641\u0627\u0637\u0645\u0629.\u0627.\u0645\"\u060c \u0648\"\u0646\u0627\u062f\u064a\u0629. \u062a.\u0645\" \u0665\u0660 \u0639\u0627\u0645\u064b\u0627\u060c \u0648\"\u0646\u0641\u064a\u0633\u0629. \u0645.\u0645\" \u0664\u0665 \u0639\u0627\u0645\u0627\u060c \u0648\"\u0646\u0639\u0645\u0629.\u064a\" \u0663\u0667 \u0639\u0627\u0645\u0627\u060c\u0648\"\u062c\u0645\u0639\u0629.\u0639.\u0627\" \u0664\u0665 \u0639\u0627\u0645\u0627\u060c \u0648\"\u0645.\u0627 \" \u0666\u0660 \u0639\u0627\u0645\u064b\u0627\u060c \u0648\"\u0645\u062d\u0645\u062f.\u064a.\u0645\" \u0663\u0667 \u0639\u0627\u0645\u064b\u0627\u060c \u0648\"\u0639\u0628\u062f \u0627\u0644\u062d\u0645\u064a\u062f.\u0627\"\u0661\u0660 \u0623\u0639\u0648\u0627\u0645\u060c \u0648\"\u0648\u0645\u0647\u0645\u0627\u062a.\u0645.\u062f\" \u0665\u0665 \u0639\u0627\u0645\u0627\u060c \u0648\"\u0627\u062d\u0645\