3b\u0438\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0441 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c, \u0432\u044b\u044f\u0441\u043d\u0438\u043b\u0438 \u0443\u0447\u0435\u043d\u044b\u0435 \u0431\u0440\u0438\u0442\u0430\u043d\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0432\u0435\u0442\u0430 \u043f\u043e \u043c\u0435\u0434\u0438\u0446\u0438\u043d\u0441\u043a\u0438\u043c \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f\u043c.\n\n\u0414\u043b\u044f \u0430\u043d\u0430\u043b\u0438\u0437\u0430 \u0431\u044b\u043b\u0438 \u0432\u0437\u044f\u0442\u044b \u0434\u0430\u043d\u043d\u044b\u0435 \u043e \u0441\u043b\u0443\u0447\u0430\u044f\u0445 \u0437\u0430\u0440\u0430\u0436\u0435\u043d\u0438\u044f COVID-19 \u0432 \u041a\u0438\u0442\u0430\u0435 \u0438 \u0435\u0449\u0435 37 \u0441\u0442\u0440\u0430\u043d\u0430\u0445 \u0437\u0430 \u0444\u0435\u0432\u0440\u0430\u043b\u044c. \u0421\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u044b \u0440\u0430\u0437\u0434\u0435\u043b\u0438\u043b\u0438 \u0437\u0430\u0431\u043e\u043b\u0435\u0432\u0448\u0438\u0445 \u043d\u0430 \u0433\u0440\u0443\u043f\u043f\u044b \u043f\u043e \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0443 \u0438 \u0441\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u043a\u043e\u044d\u0444\u0444\u0438\u0446\u0435\u043d\u0442 \u043b\u0435\u0442\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043e\u0442 \u0432\u0438\u0440\u0443\u0441\u0430.\n\n\u00ab\u041e\u0446\u0435\u043d\u043a\u0438 \u043a\u043e\u044d\u0444\u0444\u0438\u0446\u0438\u0435\u043d\u0442\u0430 \u0441\u043c\u0435\u0440\u0442\u043d\u043e\u0441\u0442\u0438 \u043e\u0442 \u0438\u043d\u0444\u0435\u043a\u0446\u0438\u0439, \u0443\u0441\u0440\u0435\u0434\u043d\u0435\u043d\u043d\u043e\u0433\u043e \u043f\u043e \u0432\u0441\u0435\u043c \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u044b\u043c \u0433\u0440\u0443\u043f\u043f\u0430\u043c, \u043a\u043e\u043b\u0435\u0431\u043b\u044e\u0442\u0441\u044f \u043e\u0442 0,2% \u0434\u043e 1,6%, \u0442\u043e\u0433\u0434\u0430 \u043a\u0430\u043a \u043e\u0446\u0435\u043d\u043a\u0438 \u043a\u043e\u044d\u0444\u0444\u0438\u0446\u0438\u0435\u043d\u0442\u0430 \u0441\u043c\u0435\u0440\u0442\u043d\u043e\u0441\u0442\u0438 \u043e\u0442 \u0438\u043d\u0444\u0435\u043a\u0446\u0438\u0439 \u0432 \u0441\u0430\u043c\u043e\u0439 \u0441\u0442\u0430\u0440\u0448\u0435\u0439 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435, \u0441\u0442\u0430\u0440\u0448\u0435 80 \u043b\u0435\u0442 \u043a\u043e\u043b\u0435\u0431\u043b\u044e\u0442\u0441\u044f \u043e\u0442 8% \u0434\u043e 36%\u00bb, - \u0441\u043a\u0430\u0437\u0430\u043d\u043e \u0432 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0438, \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043d\u043d\u043e\u043c \u0432 \u0436\u0443\u0440\u043d\u0430\u043b\u0435 The Lancet Infectious Diseases.\n\n\u0421\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430\u043c, \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c \u0443\u043c\u0435\u0440\u0435\u0442\u044c \u043e\u0442 \u043a\u043e\u0440\u043e\u043d\u0430\u0432\u0438\u0440\u0443\u0441\u0430 \u0432 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0435 \u0434\u043e 39 \u043b\u0435\u0442 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0440\u0430\u0432\u043d\u0430 \u043d\u0443\u043b\u044e, \u0432 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0435 40-49 \u043b\u0435\u0442 \u2013 0,2 %, 50-59 \u043b\u0435\u0442 \u2013 0,6 %, 60-69 \u043b\u0435\u0442 \u2013 \u0447\u0443\u0442\u044c \u043c\u0435\u043d\u0435\u0435 \u0434\u0432\u0443\u0445 \u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u043c.\n\n\u0412 \u043e\u0441\u043e\u0431\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435 \u0440\u0438\u0441\u043a\u0430 \u043d\u0430\u0445\u043e\u0434\u044f\u0442\u0441\u044f \u043b\u044e\u0434\u0438 \u0432 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0435 \u043e\u0442 70 \u0434\u043e 79 \u043b\u0435\u0442 - 4,3 % \u0438 \u0441\u0442\u0430\u0440\u0448\u0435 80 \u2013 7,8 %.\n\n\u041f\u043e \u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044e \u043d\u0430 1 \u0430\u043f\u0440\u0435\u043b\u044f \u0432\u043e \u0432\u0441\u0435\u043c \u043c\u0438\u0440\u0435 \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043e 857 442 \u0441\u043b\u0443\u0447\u0430\u0435\u0432 \u0438 42 \u0442\u044b\u0441\u044f\u0447\u0438 \u043f\u043e\u0433\u0438\u0431\u043b\u0438, \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u0432\u044b\u043b\u0435\u0447\u0438\u043b\u0438\u0441\u044c \u0431\u043e\u043b\u044c\u0448\u0435 178 \u0442\u044b\u0441\u044f\u0447 \u0447\u0435\u043b\u043e\u0432\u0435\u043a.\n\n\u042d\u043f\u0438\u0434\u0435\u043c\u0438\u044f \u043d\u0430\u0447\u0430\u043b\u0430\u0441\u044c \u0432 \u043c\u0430\u0442\u0435\u0440\u0438\u043a\u043e\u0432\u043e\u043c \u041a\u0438\u0442\u0430\u0435 \u0432 \u0432 \u0433\u043e\u0440\u043e\u0434\u0435 \u0423\u0445\u0430\u043d\u044c, \u043f\u0440\u043e\u0432\u0438\u043d\u0446\u0438\u0438 \u0425\u0443\u0431\u044d\u0439. \u041e\u0434\u043d\u0430\u043a\u043e 26 \u0444\u0435\u0432\u0440\u0430\u043b\u044f \u0442\u0435\u043c\u043f\u044b \u0440\u043e\u0441\u0442\u0430 \u0437\u0430\u0431\u043e\u043b\u0435\u0432\u0430\u0435\u043c\u043e\u0441\u0442\u0438 \u0432 \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u043e\u043c \u043c\u0438\u0440\u0435 \u0441\u0442\u0430\u043b\u0438 \u0432\u044b\u0448\u0435, \u0447\u0435\u043c \u0432 \u041a\u0438\u0442\u0430\u0435. \u0421\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u0432\u0441\u043f\u044b\u0448\u043a\u0438 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u044f\u0442 \u0432 \u0418\u0442\u0430\u043b\u0438\u0438, \u0421\u0428\u0410, \u0418\u0441\u043f\u0430\u043d\u0438\u0438, \u0438 \u0418\u0440\u0430\u043d\u0435, \u0438 \u0433\u0435\u043e\u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0440