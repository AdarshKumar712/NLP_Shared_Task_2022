u0627\u062a\u062e\u0627\u0630 \u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0644\u0627\u0632\u0645\u0629 \u0627\u0644\u062a\u064a \u0623\u0642\u0631\u062a\u0647\u0627 \u0648\u0632\u0627\u0631\u0629 \u0627\u0644\u0635\u062d\u0629 \u0648\u0627\u0644\u0633\u0643\u0627\u0646\u060c \u0641\u064a \u0627\u0644\u062a\u0639\u0627\u0645\u0644 \u0645\u0639 \u0627\u0644\u0645\u0635\u0627\u0628\u064a\u0646 \u0628\u0627\u0644\u0641\u064a\u0631\u0648\u0633\u060c \u0648\u0627\u0644\u062a\u064a \u062a\u0639\u062f\u0651 \u0623\u0647\u0645 \u0627\u0644\u0645\u0633\u0626\u0648\u0644\u064a\u0627\u062a \u0627\u0644\u0645\u0648\u0643\u0644\u0629 \u0625\u0644\u064a\u0647\u0645\u060c \u0645\u0639 \u062a\u0623\u0643\u0651\u064a\u062f \u0627\u062a\u062e\u0627\u0630 \u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0642\u0627\u0646\u0648\u0646\u064a\u0629 \u0627\u0644\u0644\u0627\u0632\u0645\u0629 \u062a\u062c\u0627\u0647 \u0645\u0646 \u064a\u062b\u0628\u062a \u062a\u0642\u0635\u064a\u0631\u0647.\n\n\u0648\u0623\u0639\u0644\u0646\u062a \u0645\u062d\u0627\u0641\u0638 \u062f\u0645\u064a\u0627\u0637\u060c \u062a\u0643\u0644\u064a\u0641 \u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0645\u062d\u0645\u062f \u0639\u0628\u062f\u0627\u0644\u0631\u062d\u0645\u0646 \u0641\u0627\u064a\u062f\u060c \u0645\u062f\u064a\u0631\u064b\u0627 \u0644\u0645\u0633\u062a\u0634\u0641\u0649 \u0635\u062f\u0631 \u062f\u0645\u064a\u0627\u0637\u060c \u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0623\u062d\u0645\u062f \u0645\u062d\u0645\u062f \u0631\u0634\u062f\u064a \u0646\u0627\u0626\u0628\u064b\u0627 \u0644\u0645\u062f\u064a\u0631 \u0627\u0644\u0645\u0633\u062a\u0634\u0641\u0649\u060c \u0648\u0627\u0644\u062f\u0643\u062a\u0648\u0631 \u0623\u062d\u0645\u062f \u0645\u062d\u0645\u062f \u0639\u0628\u062f\u0627\u0644\u0644\u0647 \u0648\u0643\u064a\u0644\u064b\u0627 \u0644\u0644\u0645\u0633\u062a\u0634\u0641\u0649.\n\n\u0648\u062a\u0636\u0645\u0646 \u0627\u0644\u0642\u0631\u0627\u0631\u060c \u0623\u0646\u0647 \u0641\u064a \u062d\u0627\u0644 \u062d\u062f\u0648\u062b \u062d\u0627\u0644\u0627\u062a \u0648\u0641\u0627\u0629 \u064a\u0634\u062a\u0628\u0647 \u0641\u0649 \u0625\u0635\u0627\u0628\u062a\u0647\u0627 \u0628\u0641\u064a\u0631\u0648\u0633 \u0643\u0648\u0631\u0648\u0646\u0627\u060c \u064a\u062a\u0645 :\n\n- \u0623\u0648\u0644\u064b\u0627: \u0627\u0644\u062a\u063a\u0633\u064a\u0644 \u062f\u0627\u062e\u0644 \u0627\u0644\u0645\u0633\u062a\u0634\u0641\u0649 \u0648\u0641\u0642\u064b\u0627 \u0644\u0644\u0628\u0631\u0648\u062a\u0648\u0643\u0648\u0644 \u0627\u0644\u0645\u0639\u062a\u0645\u062f \u0641\u0649 \u0627\u0644\u062a\u0639\u0627\u0645\u0644 \u0645\u0639 \u0647\u0630\u0647 \u0627\u0644\u062d\u0627\u0644\u0627\u062a \u0648\u064a\u0643\u0648\u0646 \u062a\u062d\u062a \u0625\u0634\u0631\u0627\u0641 \u0641\u0631\u064a\u0642 \u0645\u0643\u0627\u0641\u062d\u0629 \u0627\u0644\u0639\u062f\u0648\u0649.\n\n- \u062b\u0627\u0646\u064a\u064b\u0627: \u0627\u0644\u0627\u0644\u062a\u0632\u0627\u0645 \u0628\u0627\u0644\u0634\u0631\u062d \u0648\u0627\u0644\u062a\u062f\u0631\u064a\u0628 \u0627\u0644\u0643\u0627\u0645\u0644 \u0644\u0623\u0647\u0644 \u0627\u0644\u0645\u062a\u0648\u0641\u0649 \u0628\u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0648\u0642\u0627\u0626\u064a\u0629 \u0648\u0627\u0644\u0627\u062d\u062a\u0631\u0627\u0632\u064a\u0629 \u0644\u062c\u0645\u064a\u0639 \u0627\u0644\u0645\u062e\u0627\u0644\u0637\u064a\u0646 \u0648\u0645\u062a\u0627\u0628\u0639\u0629 \u062a\u0646\u0641\u064a\u0630 \u0630\u0644\u0643 \u0645\u0646 \u062e\u0644\u0627\u0644 \u0641\u0631\u064a\u0642 \u0627\u0644\u062a\u0631\u0635\u062f.\n\n-\u062b\u0627\u0644\u062b\u064b\u0627 \u064a\u062a\u0645 \u062a\u0633\u0644\u064a\u0645 \u0627\u0644\u062c\u062b\u0629 \u0644\u0630\u0648\u064a\u0647\u0627 \u0628\u0639\u062f \u062a\u0648\u0641\u064a\u0631 \u0639\u0631\u0628\u064a\u0629 \u0646\u0642\u0644 \u0645\u0648\u062a\u0649 \u0645\u0646\u0627\u0633\u0628\u0629.\n\n\u0648\u062c\u0627\u0621 \u0647\u0630\u0627 \u0627\u0644\u0642\u0631\u0627\u0631\u060c \u0628\u0639\u062f \u0648\u0627\u0642\u0639\u0629 \"\u0642\u0631\u064a\u0629 \u0627\u0644\u0633\u0648\u0627\u0644\u0645\"\u060c \u0648\u0627\u0644\u062a\u064a \u062a\u0642\u062f\u0645 \u0627\u0644\u0646\u0627\u0626\u0628 \u0623\u0628\u0648 \u0627\u0644\u0645\u0639\u0627\u0637\u0649 \u0645\u0635\u0637\u0641\u0649\u060c \u0628\u0637\u0644\u0628 \u0644\u0641\u062d\u0635 \u0643\u0644 \u0627\u0644\u0645\u062e\u0627\u0644\u0637\u064a\u0646 \u0644\u062d\u0627\u0644\u0629 \u0627\u0644\u0645\u062a\u0648\u0641\u064a \u0628\u0641\u064a\u0631\u0648\u0633 \u0643\u0648\u0631\u0648\u0646\u0627 \u0628\u0639\u062f\u0645\u0627 \u0623\u0643\u0651\u062f \u0623\u0647\u0644 \u0627\u0644\u0642\u0631\u064a\u0629.\n\n\u0648\u0623\u0650\u0634\u0627\u0631 \u0623\u0628\u0648 \u0627\u0644\u0645\u0639\u0627\u0637\u064a\u060c \u0625\u0644\u0649 \u0623\u0646\u0651\u064e\u0647 \u0644\u0645 \u064a\u062a\u0645\u0651 \u0627\u062a\u062e\u0627\u0630 \u0627\u0644\u0627\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0648\u0642\u0627\u0626\u064a\u0629 \u0641\u064a \u062a\u063a\u0633\u064a\u0644 \u0648\u062f\u0641\u0646 \u062c\u062b\u0645\u0627\u0646 \u0627\u0644\u0645\u062a\u0648\u0641\u0649\u060c \u0628\u0644 \u062a\u0645 \u062f\u0641\u0646\u0647 \u0628\u0637\u0631\u064a\u0642\u0629 \u0627\u0639\u062a\u064a\u0627\u062f\u064a\u0629 \u062a\u062e\u0627\u0644\u0641 \u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0645\u062a\u0628\u0639\u0629 \u0641\u064a \u062f\u0641\u0646 \u062c\u062b\u0627\u0645\u064a\u0646 \u0648\u0641\u064a\u0627\u062a \u0643\u0648\u0631\u0648\u0646\u0627.", "keywords": [], "meta_keywords": ["\u0625\u0642\u0627\u0644\u0629 \u0645\u062f\u064a\u0631 \u0645\u0633\u062a\u0634\u0641\u0649 \u0627\u0644\u0635\u062f\u0631 \u0641\u064a \u062f\u0645\u064a\u0627\u0637", "\u0645\u0633\u062a\u0634\u0641\u0649 \u0627\u0644\u0635\u062f\u0631 \u0628\u062f\u0645\u064a\u0627\u0637", "\u0645\u062d\u0627\u0641\u0