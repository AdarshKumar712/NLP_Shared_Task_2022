u0439 \u041f\u0435\u0441\u043a\u043e\u0432 \u0432\u0435\u0440\u043d\u0443\u043b\u0441\u044f \u043a \u0440\u0430\u0431\u043e\u0442\u0435. \u041e\u0431 \u044d\u0442\u043e\u043c \u0432\u043e \u0432\u0442\u043e\u0440\u043d\u0438\u043a, 26 \u043c\u0430\u044f, \u0441\u043e\u043e\u0431\u0449\u0438\u043b \u043e\u0434\u0438\u043d \u0438\u0437 \u0442\u0435\u043b\u0435\u0433\u0440\u0430\u043c-\u043a\u0430\u043d\u0430\u043b\u043e\u0432.\n\n\u041f\u043e \u0435\u0433\u043e \u0434\u0430\u043d\u043d\u044b\u043c, \u0432\u043e \u0432\u0442\u043e\u0440\u043d\u0438\u043a \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u0435\u043b\u044c \u041a\u0440\u0435\u043c\u043b\u044f \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u0442 \u043f\u0435\u0440\u0432\u044b\u0439 \u043f\u043e\u0441\u043b\u0435 \u0431\u043e\u043b\u0435\u0437\u043d\u0438 \u043a\u043e\u043d\u0444\u0435\u0440\u0435\u043d\u0446-\u043a\u043e\u043b\u043b \u0441 \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442\u0430\u043c\u0438.\n\n\u041d\u0430\u043a\u0430\u043d\u0443\u043d\u0435 \u041f\u0435\u0441\u043a\u043e\u0432\u0430 \u0432\u044b\u043f\u0438\u0441\u0430\u043b\u0438 \u0438\u0437 \u0431\u043e\u043b\u044c\u043d\u0438\u0446\u044b, \u0441\u043e\u043e\u0431\u0449\u0438\u043b\u0430 \u0435\u0433\u043e \u0436\u0435\u043d\u0430 \u0422\u0430\u0442\u044c\u044f\u043d\u0430 \u041d\u0430\u0432\u043a\u0430. \u041a\u0430\u043a \u0443\u0442\u043e\u0447\u043d\u0438\u043b\u0430 \u0444\u0438\u0433\u0443\u0440\u0438\u0441\u0442\u043a\u0430, \u043e\u043d\u0438 \u0441 \u043c\u0443\u0436\u0435\u043c \u0447\u0443\u0432\u0441\u0442\u0432\u0443\u044e\u0442 \u0441\u0435\u0431\u044f \u0445\u043e\u0440\u043e\u0448\u043e \u0438 \u043e\u043a\u043e\u043b\u043e \u043d\u0435\u0434\u0435\u043b\u0438 \u043f\u0440\u043e\u0431\u0443\u0434\u0443\u0442 \u043d\u0430 \u043a\u0430\u0440\u0430\u043d\u0442\u0438\u043d\u0435.\n\n12 \u043c\u0430\u044f \u043f\u0440\u0435\u0441\u0441-\u0441\u0435\u043a\u0440\u0435\u0442\u0430\u0440\u044c \u043f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u0430 \u0441\u043e\u043e\u0431\u0449\u0438\u043b \u043e \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u0442\u0435\u0441\u0442\u0435 \u043d\u0430 \u043a\u043e\u0440\u043e\u043d\u0430\u0432\u0438\u0440\u0443\u0441. \u0412 \u0442\u043e\u0442 \u0436\u0435 \u0434\u0435\u043d\u044c \u041d\u0430\u0432\u043a\u0430 \u0441\u043e\u043e\u0431\u0449\u0438\u043b\u0430, \u0447\u0442\u043e \u0442\u0430\u043a\u0436\u0435 \u0437\u0430\u0440\u0430\u0437\u0438\u043b\u0430\u0441\u044c \u0438\u043d\u0444\u0435\u043a\u0446\u0438\u0435\u0439, \u0441\u0443\u043f\u0440\u0443\u0433\u0438 \u043b\u0435\u0433\u043b\u0438 \u0432 \u0431\u043e\u043b\u044c\u043d\u0438\u0446\u0443.\n\n\u041d\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u0434\u0435\u043d\u044c \u041f\u0435\u0441\u043a\u043e\u0432 \u0432 \u0438\u043d\u0442\u0435\u0440\u0432\u044c\u044e \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u043b, \u0447\u0442\u043e \u043f\u0435\u0440\u0432\u044b\u0435 \u0441\u0438\u043c\u043f\u0442\u043e\u043c\u044b COVID-19 \u043f\u043e\u044f\u0432\u0438\u043b\u0438\u0441\u044c \u0443 \u043d\u0435\u0433\u043e, \u043a\u043e\u0433\u0434\u0430 \u043e\u043d \u043d\u0430\u0445\u043e\u0434\u0438\u043b\u0441\u044f \u043d\u0430 \u0441\u0430\u043c\u043e\u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438 \u043f\u043e\u0441\u043b\u0435 \u0442\u043e\u0433\u043e, \u043a\u0430\u043a \u0437\u0430\u0431\u043e\u043b\u0435\u043b \u043e\u0434\u0438\u043d \u0438\u0437 \u0435\u0433\u043e \u043a\u043e\u043b\u043b\u0435\u0433.\n\n\u0410\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u0430\u044f \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043f\u043e \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438 \u0441 \u043a\u043e\u0440\u043e\u043d\u0430\u0432\u0438\u0440\u0443\u0441\u043e\u043c \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0430 \u043d\u0430 \u0441\u0430\u0439\u0442\u0430\u0445 \u0441\u0442\u043e\u043f\u043a\u043e\u0440\u043e\u043d\u043