442\u0435\u043b\u044c\u0441\u0442\u0432\u0430.\n\n\u0410\u0440\u0441\u0435\u043d \u0410\u0432\u0430\u043a\u043e\u0432 \u0438 \u0414\u0435\u043d\u0438\u0441 \u0428\u043c\u044b\u0433\u0430\u043b\u044c\n\n\"\u042f \u0432\u0438\u0436\u0443 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u044c \u0438 \u0440\u0430\u0431\u043e\u0442\u0443 \u044d\u0442\u043e\u0433\u043e \u043c\u0438\u043d\u0438\u0441\u0442\u0435\u0440\u0441\u0442\u0432\u0430 \u0432 \u0441\u043b\u043e\u0436\u043d\u044b\u0445 \u0443\u0441\u043b\u043e\u0432\u0438\u044f\u0445 \u0434\u043b\u044f \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0435\u043d\u0438\u044f \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0432 \u0423\u043a\u0440\u0430\u0438\u043d\u0435. \u0412\u044b \u0432\u0438\u0434\u0438\u0442\u0435, \u0447\u0442\u043e \u0432 \u043c\u0438\u0440\u0435 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u044f\u0442 \u0441\u043b\u043e\u0436\u043d\u044b\u0435 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u044b \u043e\u0442 \u0421\u0428\u0410 \u0434\u043e \u0415\u0432\u0440\u043e\u043f\u044b, \u0433\u0434\u0435 \u0435\u0441\u0442\u044c \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u043d\u044b\u0435 \u0441\u043b\u043e\u0436\u043d\u043e\u0441\u0442\u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u0432 \u0441\u0432\u044f\u0437\u0438 \u0441 \u043a\u0430\u0440\u0430\u043d\u0442\u0438\u043d\u043d\u044b\u043c\u0438 \u043c\u0435\u0440\u043e\u043f\u0440\u0438\u044f\u0442\u0438\u044f\u043c\u0438 \u0438 \u043e\u0431\u0435\u0434\u043d\u0435\u043d\u0438\u0435\u043c \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u044f\", \u2013 \u043e\u0442\u043c\u0435\u0442\u0438\u043b \u043e\u043d.\n\n\u041f\u043e \u0441\u043b\u043e\u0432\u0430\u043c \u0428\u043c\u044b\u0433\u0430\u043b\u044f, \u041c\u0412\u0414 \u0441\u0435\u0439\u0447\u0430\u0441 \u043e\u0431\u044a\u0435\u0434\u0438\u043d\u044f\u0435\u0442 \u0438 \u0441\u043b\u0443\u0436\u0431\u0443 \u043f\u043e \u0447\u0440\u0435\u0437\u0432\u044b\u0447\u0430\u0439\u043d\u044b\u043c \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f\u043c, \u0438 \u043f\u043e\u0433\u0440\u0430\u043d\u0438\u0447\u043d\u0443\u044e \u0441\u043b\u0443\u0436\u0431\u0443, \u0438 \u043f\u043e\u043b\u0438\u0446\u0438\u044e, \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u0432\u0435\u0434\u043e\u043c\u0441\u0442\u0432\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0441\u0442\u043e\u044f\u0442 \u043d\u0430 \u043e\u0431\u0435\u0441\u043f\u0435\u0447\u0435\u043d\u0438\u0438 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0432 \u0423\u043a\u0440\u0430\u0438\u043d\u0435.\n\n\"\u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u0432 \u043a\u0440\u0438\u0437\u0438\u0441\u043d\u043e\u0439 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438 \u043d\u0430\u043c \u043d\u0435\u043b\u044c\u0437\u044f \u0440\u0430\u0437\u043c\u0435\u043d\u0438\u0432\u0430\u0442\u044c\u0441\u044f \u043d\u0430 \u043a\u0430\u043a\u0438\u0435-\u0442\u043e \u043f\u043e\u043b\u0438\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0432\u0435\u0449\u0438. \u0410\u0432\u0430\u043a\u043e\u0432 \u0432 \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0438 \u043a\u0440\u0438\u0437\u0438\u0441\u043e\u043c \u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0438\u0440\u0443\u0435\u0442 \u0445\u043e\u0440\u043e\u0448\u0438\u0439 \u043e\u043f\u044b\u0442 \u0438 \u0445\u043e\u0440\u043e\u0448\u0438\u0435 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430\", \u2013 \u043f\u043e\u0434\u0447\u0435\u0440\u043a\u043d\u0443\u043b \u043f\u0440\u0435\u043c\u044c\u0435\u0440.\n\n\u041a\u0430\u043a \u0441\u043e\u043e\u0431\u0449\u0430\u043b OBOZREVATEL, \u0440\u0430\u043d\u0435\u0435 \u0410\u0440\u0441\u0435\u043d \u0410\u0432\u0430\u043a\u043e\u0432 \u043f\u0440\u043e\u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u043b \u0441\u0431\u043e\u0440 \u043f\u043e\u0434\u043f\u0438\u0441\u0435\u0439 \u04