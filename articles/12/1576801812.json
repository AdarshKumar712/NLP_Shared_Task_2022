u0627\u0631\u0629 \u0627\u0644\u0646\u0642\u0644 \u0627\u0644\u0639\u0631\u0627\u0642\u064a\u0629\u060c \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u0627\u062b\u0646\u064a\u0646\u060c \u0627\u0646\u0637\u0644\u0627\u0642 \u0627\u0644\u0637\u0627\u0626\u0631\u0629 \u0637\u0631\u0627\u0632 A320/YI-ARA \u0627\u0644\u062a\u0627\u0628\u0639\u0629 \u0644\u0623\u0633\u0637\u0648\u0644 \u0627\u0644\u0646\u0627\u0642\u0644 \u0627\u0644\u0648\u0637\u0646\u064a \u0627\u0644\u0639\u0631\u0627\u0642\u064a \u0645\u0646 \u0645\u0637\u0627\u0631 \u0628\u063a\u062f\u0627\u062f \u0627\u0644\u062f\u0648\u0644\u0649 \u0645\u062a\u0648\u062c\u0647\u0629 \u0625\u0644\u0649 \u0645\u0637\u0627\u0631 \u062c\u062f\u0629 \u0628\u0627\u0644\u0633\u0639\u0648\u062f\u064a\u0629\u060c \u0648\u0623\u0641\u0627\u062f\u062a \u0627\u0644\u0648\u0643\u0627\u0644\u0629 \u0627\u0644\u0648\u0637\u0646\u064a\u0629 \u0627\u0644\u0639\u0631\u0627\u0642\u064a\u0629 \u0644\u0644\u0623\u0646\u0628\u0627\u0621 \"\u0646\u064a\u0646\u0627\"\u060c \u0623\u0646\u0647\u0627 \u062a\u0639\u062f \u0627\u0644\u0631\u062d\u0644\u0629 \u0627\u0644\u0627\u0633\u062a\u062b\u0646\u0627\u0626\u064a\u0629 \u0627\u0644\u0623\u0648\u0644\u0649 \u0627\u0644\u062e\u0627\u0635\u0629 \u0628\u0625\u0639\u0627\u062f\u0629 \u0627\u0644\u0639\u0631\u0627\u0642\u064a\u064a\u0646 \u0627\u0644\u0639\u0627\u0644\u0642\u064a\u0646 \u0641\u064a \u0627\u0644\u0645\u0645\u0644\u0643\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0627\u0644\u0633\u0639\u0648\u062f\u064a\u0629.\n\n\u0648\u0643\u0627\u0646\u062a \u062f\u0627\u0626\u0631\u0629 \u0635\u062d\u0629 \u0627\u0644\u0631\u0635\u0627\u0641\u0629 \u0628\u0628\u063a\u062f\u0627\u062f\u060c\u0623\u0639\u0644\u0646\u062a \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u0627\u062b\u0646\u064a\u0646\u060c \u062a\u0633\u062c\u064a\u0644 25 \u062d\u0627\u0644\u0629 \u0634\u0641\u0627\u0621 \u062c\u062f\u064a\u062f\u0629 \u0645\u0646 \u0641\u064a\u0631\u0648\u0633 \u0643\u0648\u0631\u0646\u0627 \u062e\u0644\u0627\u0644 \u064a\u0648\u0645 \u0648\u0627\u062d\u062f\u060c \u0644\u064a\u0631\u062a\u0641\u0639 \u0627\u0644\u0639\u062f\u062f \u0627\u0644\u062a\u0631\u0627\u0643\u0645\u064a \u0644\u062d\u0627\u0644\u0627\u062a \u0627\u0644\u0634\u0641\u0627\u0621 \u0625\u0644\u0649 90 \u062d\u0627\u0644\u0629 \u0634\u0641\u0627\u0621 \u0641\u0649 \u0627\u0644\u0631\u0635\u0627\u0641\u0629\u060c \u0648\u0642\u0627\u0644 \u0645\u062f\u064a\u0631 \u0639\u0627\u0645 \u062f\u0627\u0626\u0631\u0629 \u0635\u062d\u0629 \u0627\u0644\u0631\u0635\u0627\u0641\u0629\u060c \u0639\u0628\u062f \u0627\u0644\u063a\u0646\u064a \u0627\u0644\u0633\u0627\u0639\u062f\u064a - \u0641\u064a \u0628\u064a\u0627\u0646 \u0623\u0648\u0631\u062f\u062a\u0647 \u0627\u0644\u0648\u0643\u0627\u0644\u0629 \u0627\u0644\u0648\u0637\u0646\u064a\u0629 \u0627\u0644\u0639\u0631\u0627\u0642\u064a\u0629 \u0644\u0644\u0623\u0646\u0628\u0627\u0621 \"\u0646\u064a\u0646\u0627\" - \"\u0646\u0639\u0644\u0646 \u0634\u0641\u0627\u0621 25 \u0645\u0635\u0627\u0628\u0627\u064b \u0628\u0641\u064a\u0631\u0648\u0633 \u0643\u0648\u0631\u0648\u0646\u0627 \u0641\u064a \u0645\u0633\u062a\u0634\u0641\u0649 \u0627\u0628\u0646 \u0627\u0644\u062e\u0637\u064a\u0628 \u0648\u0647\u064a \u0623\u0639\u0644\u0649 \u0646\u0633\u0628\u0629 \u062a\u0639\u0627\u0641 \u0644\u0645\u0631\u0636\u0649 \u064a\u062a\u0645 \u0634\u0641\u0627\u0624\u0647\u0645 \u0641\u064a \u064a\u0648\u0645 \u0648\u0627\u062d\u062f\u060c \u0628\u0641\u0636\u0644 \u062c\u0647\u0648\u062f \u0627\u0644\u0641\u0631\u064a\u0642 \u0627\u0644\u0637\u0628\u064a \u0628\u0627\u0644\u0645\u0633\u062a\u0634\u0641\u0649 \u0648\u0643\u0644 \u0627\u0644\u0645\u0624\u0633\u0633\u0627\u062a \u0627\u0644\u0635\u062d\u064a\u0629\".", "keywords": [], "meta_keywords": ["\u0627\u0644\u0639\u0631\u0627\u0642", "\u0627\u0644\u0633\u0639\u0648\u062f\u064a\u0647", "\u062c\u062f\u0647", "\u0627\u0644\u0639\u0627\u0644\u0642\u064a\u0646", "\u0627\u0644\u062e\u0637\u0648\u0637 \u0627\u0644\u062c\