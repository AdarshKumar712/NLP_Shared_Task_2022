u043d\u0435 \u043f\u043e\u043b\u0438\u0446\u0435\u0439\u0441\u043a\u0438\u0435 \u0437\u0430\u0434\u0435\u0440\u0436\u0430\u043b\u0438 \u043f\u043e\u0434\u043e\u0437\u0440\u0435\u0432\u0430\u0435\u043c\u044b\u0445 \u0432 \u0440\u0430\u0437\u0431\u043e\u0439\u043d\u043e\u043c \u043d\u0430\u043f\u0430\u0434\u0435\u043d\u0438\u0438.\n\n\u041a\u0430\u043a \u0441\u043e\u043e\u0431\u0449\u0438\u043b\u0438 \u0432 \u043f\u0440\u0435\u0441\u0441-\u0441\u043b\u0443\u0436\u0431\u0435 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0443\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u041c\u0412\u0414, \u0432 \u043f\u043e\u043b\u0438\u0446\u0438\u044e \u041a\u0440\u0430\u0441\u043d\u043e\u0430\u0440\u043c\u0435\u0439\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430 \u043e\u0431\u0440\u0430\u0442\u0438\u043b\u0441\u044f \u043c\u0435\u0441\u0442\u043d\u044b\u0439 \u0436\u0438\u0442\u0435\u043b\u044c. \u041e\u043d \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u043b, \u0447\u0442\u043e \u0432 \u0435\u0433\u043e \u0434\u043e\u043c \u0432\u043e\u0440\u0432\u0430\u043b\u0438\u0441\u044c \u0434\u0432\u043e\u0435 \u043d\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445, \u043d\u0430\u043f\u0430\u043b\u0438 \u043d\u0430 \u043d\u0435\u0433\u043e, \u0438, \u0443\u0433\u0440\u043e\u0436\u0430\u044f \u043d\u043e\u0436\u043e\u043c, \u043f\u043e\u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043b\u0438 \u0434\u0435\u043d\u044c\u0433\u0438.\n\n\u041c\u0443\u0436\u0447\u0438\u043d\u044b \u0437\u0430\u0431\u0440\u0430\u043b\u0438 \u0442\u0440\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430, \u0444\u043b\u0435\u0448-\u043a\u0430\u0440\u0442\u0443, \u0431\u0438\u0436\u0443\u0442\u0435\u0440\u0438\u044e \u0438 40 \u043b\u0438\u0442\u0440\u043e\u0432 \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u044f \u0434\u043e\u043c\u0430\u0448\u043d\u0435\u0433\u043e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0441\u0442\u0432\u0430, \u043f\u043e\u0441\u043b\u0435 \u0447\u0435\u0433\u043e \u0441\u043a\u0440\u044b\u043b\u0438\u0441\u044c. \u0421\u0443\u043c\u043c\u0430 \u0443\u0449\u0435\u0440\u0431\u0430 \u0441\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0430 \u043e\u043a\u043e\u043b\u043e 20 \u0442\u044b\u0441. \u0440\u0443\u0431\u043b\u0435\u0439.\n\n\u041f\u0440\u0430\u0432\u043e\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u0435\u043b\u0438 \u043f\u043e \u0433\u043e\u0440\u044f\u0447\u0438\u043c \u0441\u043b\u0435\u0434\u0430\u043c \u0437\u0430\u0434\u0435\u0440\u0436\u0430\u043b\u0438 \u0440\u0430\u043d\u0435\u0435 \u0441\u0443\u0434\u0438\u043c\u044b\u0445 28-\u043b\u0435\u0442\u043d\u0435\u0433\u043e \u043c\u0435\u0441\u0442\u043d\u043e\u0433\u043e \u0436\u0438\u0442\u0435\u043b\u044f \u0438 31-\u043b\u0435\u0442\u043d\u0435\u0433\u043e \u043f\u0440\u0438\u0435\u0437\u0436\u0435\u0433\u043e \u0438\u0437 \u041d\u043e\u0432\u043e\u043f\u043e\u043a\u0440\u043e\u0432\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430.\n\n\u0412 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438 \u043f\u043e\u0434\u043e\u0437\u0440\u0435\u0432\u0430\u0435\u043c\u044b\u0445 \u0432\u043e\u0437\u0431\u0443\u0436\u0434\u0435\u043d\u043e \u0443\u0433\u043e\u043b\u043e\u0432\u043d\u043e\u0435 \u0434\u0435\u043b\u043e \u043e \u0440\u0430\u0437\u0431\u043e\u0435. \u041c\u0443\u0436\u0447\u0438\u043d\u044b \u0430\u0440\u0435\u0441\u0442\u043e\u0432\u0430\u043d\u044b, \u0438\u043c \u0433\u0440\u043e\u0437\u0438\u0442 \u0434\u043e 12 \u043b\u0435\u0442 \u043b\u0438\u0448\u0435\u043d\u0438\u044f \u0441\u0432\u043e\u0431\u043e\u0434\u044b.\n\n\u041a\u0430\u043a \u0440\u0430\u043d\u0435\u0435 \u0441\u043e\u043e\u0431\u0449\u0430\u043b\u0438 \u00ab\u041a\u0443\u0431\u0430\u043d\u0441\u043a\u0438\u0435 \u043d\u043e\u0432\u043e\u0441\u0442\u0438\u00bb, \u0432 \u0422\u0438\u0445\u043e\u0440\u0435\u0446\u043a\u0435 \u0432 \u043c\u0430\u0433\u0430\u0437\u0438\u043d \u0432\u043e\u0448\u043b\u0430 \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u044c\u043d\u0438\u0446\u0430 \u0438 \u043f\u043e\u043f\u0440\u043e\u0441\u0438\u043b\u0430 \u0432\u0437\u0432\u0435\u0441\u0438\u0442\u044c \u043a\u043e\u043b\u0431\u0430\u0441\u0443. \u041a\u043e\u0433\u0434\u0430 \u043f\u0440\u043e\u0434\u0430\u0432\u0435\u0446 \u043e\u0437\u0432\u0443\u0447\u0438\u043b\u0430 \u0441\u0443\u043c\u043c\u0443 \u043f\u043e\u043a\u0443\u043f\u043a\u0438, \u0437\u043b\u043e\u0443\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u0438\u0446\u0430 \u0434\u043e\u0441\u0442\u0430\u043b\u0430 \u0438\u0437 \u043a\u0430\u0440\u043c\u0430\u043d\u0430 \u043a\u0443\u0440\u0442\u043a\u0438 \u0448\u043f\u0440\u0438\u0446 \u0441 \u0438\u0433\u043b\u043e\u0439, \u043d\u0430\u043f\u0430\u043b\u0430 \u043d\u0430 \u043d\u0435\u0435, \u043d\u0430\u043d\u0435\u0441\u043b\u0430 \u0435\u0439 \u0443\u0434\u0430\u0440\u044b \u0432 \u0433\u0440\u0443\u0434\u044c, \u043f\u043e\u0445\u0438\u0442\u0438\u043b\u0430 \u043f\u0430\u043a\u0435\u0442 \u0441 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430\u043c\u0438 \u043d\u0430 \u0441\u0443\u043c\u043c\u0443 600 \u0440\u0443\u0431\u043b\u0435\u0439 \u0438 \u0441\u043a\u0440\u044b\u043b\u0430\u0441\u044c.\n\n\u041f\u0440\u0430\u0432\u043e\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u0435\u043b\u0438 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043b\u0438 \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u044c \u0438 \u043c\u0435\u0441\u0442\u043e\u043d\u0430\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u0435 \u043f\u043e\u0434\u043e\u0437\u0440\u0435\u0432\u0430\u0435\u043c\u043e\u0439. 46-\u043b\u0435\u0442\u043d\u044e\u044e \u043c\u0435\u0441\u0442\u043d\u0443\u044e \u0436\u0438\u0442\u0435\u043b\u044c\u043d\u0438\u0446\u0443 \u043f\u0440\u0430\u0432\u043e\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u0435\u043b\u0438 \u0437\u0430\u0434\u0435\u0440\u0436\u0430\u043b\u0438 \u043f\u043e \u0433\u043e\u0440\u044f\u0447\u0438\u043c \u0441\u043b\u043