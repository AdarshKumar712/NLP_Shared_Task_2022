u043b \u043d\u0430\u0443\u0447\u043d\u044b\u0439 \u0441\u0435\u043c\u0438\u043d\u0430\u0440 \u00ab\u0410\u0431\u0430\u0439 \u2013 \u043f\u043e\u044d\u0442, \u043c\u044b\u0441\u043b\u0438\u0442\u0435\u043b\u044c, \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0438\u043a\u00bb, \u043f\u0435\u0440\u0435\u0434\u0430\u0435\u0442 \u041c\u0418\u0410 \u00ab\u041a\u0430\u0437\u0438\u043d\u0444\u043e\u0440\u043c\u00bb \u0441\u043e \u0441\u0441\u044b\u043b\u043a\u043e\u0439 \u043d\u0430 \u043f\u0440\u0435\u0441\u0441-\u0441\u043b\u0443\u0436\u0431 \u041a\u0413\u0423 \u00ab\u049a\u043e\u0493\u0430\u043c\u0434\u044b\u049b \u043a\u0435\u043b\u0456\u0441\u0456\u043c\u00bb \u043f\u0440\u0438 \u0430\u043a\u0438\u043c\u0430\u0442\u0435 \u0433\u043e\u0440\u043e\u0434\u0430 \u041d\u0443\u0440-\u0421\u0443\u043b\u0442\u0430\u043d.\n\n\u041f\u0435\u0440\u0432\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u0441\u0435\u043c\u0438\u043d\u0430\u0440\u0430 \u043f\u0440\u043e\u0448\u043b\u0430 \u0432 \u041a\u0430\u0437\u0430\u0445\u0441\u043a\u043e\u043c \u0430\u0433\u0440\u043e\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0435 \u0438\u043c. \u0421. \u0421\u0435\u0439\u0444\u0443\u043b\u043b\u0438\u043d\u0430, \u0432\u0442\u043e\u0440\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u2013 \u0432 \u0414\u043e\u043c\u0435 \u0434\u0440\u0443\u0436\u0431\u044b.\n\n\n\n\n\n\u0412 \u0441\u0432\u043e\u0435\u043c \u0432\u0441\u0442\u0443\u043f\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u043c \u0441\u043b\u043e\u0432\u0435 \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0430\u044f \u0421\u0435\u043a\u0440\u0435\u0442\u0430\u0440\u0438\u0430\u0442\u043e\u043c \u0410\u0441\u0441\u0430\u043c\u0431\u043b\u0435\u0438 \u043d\u0430\u0440\u043e\u0434\u0430 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0430 \u0433\u043e\u0440\u043e\u0434\u0430 \u041d\u0443\u0440-\u0421\u0443\u043b\u0442\u0430\u043d \u041b\u044f\u0437\u0437\u0430\u0442 \u041a\u0443\u0441\u0430\u0438\u043d\u043e\u0432\u0430 \u043e\u0442\u043c\u0435\u0442\u0438\u043b\u0430 \u0432\u0430\u0436\u043d\u043e\u0441\u0442\u044c \u0441\u0442\u0430\u0442\u044c\u0438 \u041f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u0430 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d \u041a\u0430\u0441\u044b\u043c-\u0416\u043e\u043c\u0430\u0440\u0442\u0430 \u0422\u043e\u043a\u0430\u0435\u0432\u0430 \u00ab\u0410\u0431\u0430\u0439 \u0438 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d \u0432 XXI \u0432\u0435\u043a\u0435\u00bb.\n\n\u00ab\u0412 2020 \u0433\u043e\u0434\u0443 \u043b\u0435\u0439\u0442\u043c\u043e\u0442\u0438\u0432\u043e\u043c \u043f\u0440\u043e\u0435\u043a\u0442\u0430 \u0410\u0441\u0441\u0430\u043c\u0431\u043b\u0435\u0438 \u043d\u0430\u0440\u043e\u0434\u0430 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0430 \u00ab\u049a\u0430\u0437\u0430\u049b\u0442\u0430\u043d\u0443\u00bb \u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u0441\u044f 175-\u043b\u0435\u0442\u0438\u0435 \u0432\u0435\u043b\u0438\u043a\u043e\u0433\u043e \u0410\u0431\u0430\u044f, \u043c\u044b\u0441\u043b\u0438\u0442\u0435\u043b\u044f, \u043d\u0430\u0448\u0435\u0433\u043e \u0434\u0443\u0445\u043e\u0432\u043d\u043e\u0433\u043e \u0443\u0447\u0438\u0442\u0435\u043b\u044f. \u0424\u0438\u043b\u043e\u0441\u043e\u0444\u0438\u044f \u0410\u0431\u0430\u044f \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u0430 \u043f\u043e \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0435\u0435 \u0432\u0440\u0435\u043c\u044f, \u043e\u043d\u0430 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0430 \u043d\u0430 \u0433\u0443\u043c\u0430\u043d\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0432\u043e\u0441\u043f\u0438\u0442\u0430\u043d\u0438\u0435 \u0447\u043b\u0435\u043d\u043e\u0432 \u043d\u0430\u0448\u0435\u0433\u043e \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0430. \u00ab\u041d\u0435 \u0445\u0432\u0430\u043b\u0438\u0441\u044c, \u043f\u043e\u043a\u0430 \u043d\u0435 \u043f\u043e\u0441\u0442\u0438\u0433 \u043d\u0430\u0443\u043a\u0443\u00bb, \u0443\u0447\u0438\u043b \u0432\u0435\u043b\u0438\u043a\u0438\u0439 \u0410\u0431\u0430\u0439. \u00ab\u041f\u0440\u0435\u0436\u0434\u0435 \u0432\u0441\u0435\u0433\u043e \u043d\u0443\u0436\u043d\u043e \u0441\u0442\u0430\u0442\u044c \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043c\u00bb, \u043f\u0440\u0438\u0437\u044b\u0432\u0430\u043b \u043d\u0430\u0448 \u0434\u0443\u0445\u043e\u0432\u043d\u044b\u0439 \u0443\u0447\u0438\u0442\u0435\u043b\u044c. \u0412\u0435\u043b\u0438\u043a\u0438\u0439 \u0410\u0431\u0430\u0439 \u0432 \u0441\u0432\u043e\u0435\u0439 \u0444\u0438\u043b\u043e\u0441\u043e\u0444\u0438\u0438 \u043f\u0440\u0438\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u043b\u0441\u044f \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0430: \u00ab\u041b\u044e\u0431\u0438 \u0432\u0441\u0435 \u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u0441\u0442\u0432\u043e, \u0441\u0447\u0438\u0442\u0430\u044f \u0435\u0433\u043e \u0440\u043e\u0434\u043d\u044b\u043c\u00bb. \u0412 \u043f\u0435\u0440\u0438\u043e\u0434 \u0434\u0443\u0445\u043e\u0432\u043d\u043e\u0439 \u043c\u043e\u0434\u0435\u0440\u043d\u0438\u0437\u0430\u0446\u0438\u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0430 \u0443\u0447\u0435\u043d\u0438\u0435 \u0432\u0435\u043b\u0438\u043a\u043e\u0433\u043e \u043c\u044b\u0441\u043b\u0438\u0442\u0435\u043b\u044f \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u0442\u0430\u0435\u0442 \u043e\u0441\u043e\u0431\u043e\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435: \u0421\u043b\u043e\u0432\u0430 \u043d\u0430\u0437\u0438\u0434\u0430\u043d\u0438\u044f, \u043f\u043e\u044d\u0437\u0438\u044f, \u043f\u0435\u0440\u0435\u0432\u043e\u0434\u044b - \u0432\u0441\u0435 \u0442\u0432\u043e\u0440\u0447\u0435\u0441\u0442\u0432\u043e \u0432\u0435\u043b\u0438\u043a\u043e\u0433\u043e \u0430\u043a\u044b\u043d\u0430 \u043f\u0440\u043e\u043d\u0438\u0437\u0430\u043d\u043e \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043b\u044e\u0431\u0438\u0435\u043c, \u0442\u043e\u043b\u0435\u0440\u0430\u043d\u0442\u043d\u043e\u0441\u0442\u044c\u044e, \u0434\u043e\u0431\u0440\u043e\u0442\u043e\u0439\u00bb, - \u043f\u043e\u0434\u0447\u0435\u0440\u043a\u043d\u0443\u043b\u0430 \u041b.\u041a\u0443\u0441\u0430\u0438\u043d\u043e\u0432\u0430.\n\n\n\n\n\n\u041f\u0440\u043e\u0444\u0435\u0441\u0441\u043e\u0440 \u043a\u0430\u0444\u0435\u0434\u0440\u044b \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0430 \u041a\u0430\u0437\u0430\u0445\u0441\u043a\u043e\u0433\u043e \u0430\u0433\u0440\u043e\u0442\u0435\u0445\u043d\u0438\u0447\u0435\u0441\u043a\u043e\u0433\u043e \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0438\u0442\u0435\u0442\u0430 \u0438\u043c\u0435\u043d\u0438 \u0421. \u0421\u0435\u0439\u0444\u0443\u043b\u043b\u0438\u043d\u0430 \u041a\u0430\u0439\u044b\u0440\u0431\u043e\u043b\u0430\u0442 \u041d\u0443\u0440\u0431\u0430\u0439 \u043e\u0441\u043e\u0431\u043e \u043e\u0442\u043c\u0435\u0442\u0438\u043b \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u0438\u0435 \u0442\u0440\u0443\u0434\u044b \u0410\u0431\u0430\u044f.\n\n\u00ab\u041c\u044b \u0437\u043d\u0430\u0435\u043c \u0432\u0435\u043b\u0438\u043a\u043e\u0433\u043e \u0410\u0431\u0430\u044f \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u043f\u043e\u044d\u0442\u0430 \u0438 \u043f\u0440\u043e\u0441\u0432\u0435\u0442\u0438\u0442\u0435\u043b\u044f. \u041d\u043e \u0442\u0440\u0435\u0431\u0443\u044e\u0442 \u0448\u0438\u0440\u043e\u043a\u043e\u0433\u043e \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0438 \u0435\u0433\u043e \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u0438\u0435 \u0442\u0440\u0443\u0434\u044b. \u0412 1878 \u0433\u043e\u0434\u0443 \u0431\u044b\u043b \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d \u0421\u0435\u043c\u0438\u043f\u0430\u043b\u0430\u0442\u0438\u043d\u0441\u043a\u0438\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u043d\u043e\u0439 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u043a\u043e\u043c\u0438\u0442\u0435\u0442, \u0447\u043b\u0435\u043d\u043e\u043c \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u0431\u044b\u043b \u0410\u0431\u0430\u0439 \u0438 \u0432\u043d\u0435\u0441 \u0437\u043d\u0430\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0432\u043a\u043b\u0430\u0434 \u0432 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0435\u043d\u0438\u044f, \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0438 \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u043a\u0430\u0437\u0430\u0445\u0441\u043a\u043e\u0433\u043e \u043d\u0430\u0440\u043e\u0434\u0430. \u041e\u0442\u0440\u0430\u0434\u043d\u043e, \u0447\u0442\u043e \u0432 \u0441\u0432\u043e\u0435\u0439 \u0441\u0442\u0430\u0442\u044c\u0435 \u041f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442 \u0443\u043a\u0430\u0437\u0430\u043b \u043d\u0430 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u044c \u0434\u0430\u043b\u044c\u043d\u0435\u0439\u0448\u0435\u0433\u043e \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f \u0442\u0440\u0443\u0434\u043e\u0432 \u0410\u0431\u0430\u044f, \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u043f\u0430\u0433\u0430\u043d\u0434\u044b \u0435\u0433\u043e \u0438\u0434\u0435\u0439 \u0438 \u0432 \u041a\u0430\u0437\u0430\u0445\u0441\u0442\u0430\u043d\u0435, \u0438 \u0437\u0430 \u0440\u0443\u0431\u0435\u0436\u043e\u043c. \u0410\u0431\u0430\u0439 \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0431\u043e\u043b\u044c\u0448\u043e\u0435 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u043e\u0435 \u043d\u0430\u0441\u043b\u0435\u0434\u0438\u0435, \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0438\u043d\u0442\u0435\u0440\u0435\u0441 \u0434\u043b\u044f \u0444\u0438\u043b\u043e\u043b\u043e\u0433\u043e\u0432, \u0438\u0441\u0442\u043e\u0440\u0438\u043a\u043e\u0432, \u0444\u0438\u043b\u043e\u0441\u043e\u0444\u043e\u0432. \u042d\u0442\u043e \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u043e \u0432 \u0440\u0430\u043c\u043a\u0430\u0445 \u0440\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b \u00ab\u0420\u0443\u0445\u0430\u043d\u0438 \u0436\u0430\u04a3\u0493\u044b\u0440\u0443\u00bb, - \u0441\u0447\u0438\u0442\u0430\u0435\u0442 \u041a.\u041d\u0443\u0440\u0431\u0430\u0439.\n\n\u041c\u0435\u0440\u043e\u043f\u0440\u0438\u044f\u0442\u0438\u0435 \u0442\u0430\u043a\u0436\u0435 \u0432\u043a\u043b\u044e\u0447\u0430\u043b\u043e \u0432 \u0441\u0435\u0431\u044f \u0442\u0430\u043a\u0438\u0435 \u0442\u0435\u043c\u044b, \u043a\u0430\u043a \u00ab\u041f\u0435\u0440\u0435\u0432\u043e\u0434\u0447\u0435\u0441\u043a\u0430\u044f \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0410\u0431\u0430\u044f\u00bb, \u043a\u043e\u043d\u0446\u0435\u043f\u0442 \u00ab\u0427\u0435\u043b\u043e\u0432\u0435\u043a\u0430\u00bb \u0432 \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d\u0438\u044f\u0445 \u0410\u0431\u0430\u044f\u00bb \u0438 \u0434\u0440\u0443\u0433\u0438\u0435. \u0421 \u0434\u043e\u043a\u043b\u0430\u0434\u0430\u043c\u0438 \u0432\u044b\u0441\u0442\u0443\u043f\u0438\u043b\u0438 \u043f\u0440\u0435\u0434\u0441\u0435\u0434\u0430\u0442\u0435\u043b\u044c \u043d\u0430\u0443\u0447\u043d\u043e-\u044d\u043a\u0441\u043f\u0435\u0440\u0442\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u044b \u0410\u041d\u041a \u0433. \u041d\u0443\u0440-\u0421\u0443\u043b\u0442\u0430\u043d \u0425\u0430\u043b\u0438\u043b \u